<Window x:Class="RevitProjectDataAddin.柱の配置"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:RevitProjectDataAddin"
        mc:Ignorable="d"
        Title="柱リストを入力"
        Height="auto" 
        Width="1550" 
        ResizeMode="NoResize"

        FontFamily="Yu Mincho">
    <!-- Định nghĩa tài nguyên màu -->
    <Window.Resources>
        <SolidColorBrush x:Key="TitleColor1" Color="Blue"/>
        <SolidColorBrush x:Key="TitleColor2" Color="Orange"/>
        <SolidColorBrush x:Key="Label" Color="Green"/>
        <SolidColorBrush x:Key="注意" Color="Red"/>
    </Window.Resources>
    <Grid Margin="10">
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
            <Grid>
                <Grid x:Name="GridTop" Height="2652" VerticalAlignment="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="500"/>
                        <ColumnDefinition Width="500"/>
                        <ColumnDefinition Width="500"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock HorizontalAlignment="Center" FontSize="40" Text="柱の配置" VerticalAlignment="Top" Margin="0,10,0,0" Grid.ColumnSpan="3"/>

                    <!-- 柱全断面のテキストをチェックボックスの状態で切り替え -->
                    <TextBlock FontSize="35" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,62,0,0">
                        <TextBlock.Style>
                            <Style TargetType="TextBlock">
                                <Setter Property="Text" Value="柱脚"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=柱全断面, Path=IsChecked}" Value="True">
                                        <Setter Property="Text" Value="柱全断面"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBlock.Style>
                    </TextBlock>
                    <TextBlock FontSize="35" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,62,0,0" Grid.Column="1">
                        <TextBlock.Style>
                            <Style TargetType="TextBlock">
                                <Setter Property="Text" Value="柱頭"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=柱全断面, Path=IsChecked}" Value="True">
                                        <Setter Property="Text" Value="柱全断面"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </TextBlock.Style>
                    </TextBlock>

                    <CheckBox x:Name="柱全断面" Height="15" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,79,0,0" Grid.Column="2" IsChecked="{Binding 柱全断面, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                        <ContentControl Content="柱全断面" FontSize="18" Margin=" 0,-5"/>
                    </CheckBox>

                    <!-- Grid.Column="0" の入力項目（変更なし） -->
                    <StackPanel Margin="10,110,10,0" VerticalAlignment="Top">
                        <TextBlock Text="柱幅" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                        <TextBox x:Name="柱幅" FontSize="18" HorizontalAlignment="Left" Height="32" Width="480" PreviewTextInput="TEXTBOX_PreviewTextInput" Text="{Binding 柱幅, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                    </StackPanel>
                    <StackPanel Margin="10,170,10,0" VerticalAlignment="Top">
                        <TextBlock Text="柱成" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                        <TextBox x:Name="柱成" FontSize="18" HorizontalAlignment="Left" Height="32" Width="480" PreviewTextInput="TEXTBOX_PreviewTextInput" Text="{Binding 柱成, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                    </StackPanel>
                    <StackPanel Margin="10,230,255,0" VerticalAlignment="Top">
                        <TextBlock Text="主筋径" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                        <ComboBox FontSize="18" x:Name="主筋径" Height="32" HorizontalAlignment="Left" Width="235"
  ItemsSource="{Binding Valid径Values, RelativeSource={RelativeSource AncestorType={x:Type Window}}}"
  SelectedItem="{Binding 主筋径, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                    </StackPanel>
                    <StackPanel Margin="260,230,10,0" VerticalAlignment="Top">
                        <TextBlock Text="材質" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                        <ComboBox FontSize="18" x:Name="材質" HorizontalAlignment="Left" Width="230" Height="32" ItemsSource="{Binding Valid材質Values, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" SelectedItem="{Binding 主筋材質, UpdateSourceTrigger=PropertyChanged}"/>
                    </StackPanel>
                    <StackPanel Margin="10,290,255,0" VerticalAlignment="Top">
                        <TextBlock Text="芯筋径" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                        <ComboBox FontSize="18" x:Name="芯筋径" Height="32" HorizontalAlignment="Left" Width="235"
  ItemsSource="{Binding Valid径Values, RelativeSource={RelativeSource AncestorType={x:Type Window}}}"
  SelectedItem="{Binding 芯筋径, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                    </StackPanel>
                    <StackPanel Margin="260,290,10,0" VerticalAlignment="Top">
                        <TextBlock Text="材質" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
