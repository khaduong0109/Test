<Window x:Class="RevitProjectDataAddin.計算値設定"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:RevitProjectDataAddin"
        Title="計算値設定"
        Height="auto" Width="auto" ResizeMode="NoResize" FontFamily="Yu Mincho">
    <!-- Định nghĩa tài nguyên màu -->
    <Window.Resources>
        <SolidColorBrush x:Key="TitleColor1" Color="Blue"/>
        <SolidColorBrush x:Key="TitleColor2" Color="Orange"/>
        <SolidColorBrush x:Key="Label" Color="Green"/>
        <SolidColorBrush x:Key="注意" Color="Red"/>

    </Window.Resources>
    <Grid Background="White">
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
            <Grid Margin="10" Height="auto">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" MinHeight="4779"/>
                    <RowDefinition/>
                    <RowDefinition Height="100"/>
                </Grid.RowDefinitions>
                <TextBlock Grid.Row="0" Text="一般梁 全般" FontSize="30" Foreground="{StaticResource TitleColor1}" HorizontalAlignment="Left" Margin="10,10,0,0"  VerticalAlignment="Top" />
                <TextBlock Foreground="{StaticResource Label}" Grid.Row="0" Text="継手 上筋" FontSize="20" Margin="10,81,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <ComboBox x:Name="ComboBoxUwa" 
          ItemsSource="{Binding ValidValues, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" 
          SelectedItem="{Binding Kesan.Tsuuwa, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
          SelectionChanged="ComboBoxUwa_SelectionChanged" 
          Grid.Row="0" 
          Margin="10,113,10,0" 
          FontSize="18" 
          VerticalAlignment="Top" 
          Height="30" />
                <TextBlock Foreground="{StaticResource Label}" Grid.Row="0" Text="継手 下筋" FontSize="20" Margin="10,150,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />

                <ComboBox x:Name="ComboBoxShita" 
          ItemsSource="{Binding ValidValues, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" 
          SelectedItem="{Binding Kesan.Tsushita, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
          SelectionChanged="ComboBoxShita_SelectionChanged" 
          Margin="10,182,10,0" 
          FontSize="18" 
          VerticalAlignment="Top" 
          Height="30" />
                <TextBlock Foreground="{StaticResource Label}" Text="継手長 主筋1" FontSize="20" Margin="10,222,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="TsuNaga1TextBox" Tag="TsuNaga1" Text="{Binding Kesan.Tsunaga1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,254,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Text="継手長 主筋2" FontSize="20" Margin="10,291,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="TsuNaga2TextBox" Tag="TsuNaga2" Text="{Binding Kesan.Tsunaga2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,323,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Text="定着 上筋(単位:d)" FontSize="20" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,357,0,0" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="TeichakuUwaTextBox" Tag="TeichakuUwa" Text="{Binding Kesan.TeichakuUwa, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,389,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Text="定着 下筋(単位:d)" FontSize="20" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,426,0,0"/>
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="TeichakuShitaTextBox" Tag="TeichakuShita" Text="{Binding Kesan.TeichakuShita, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,458,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Text="にげ 上筋" FontSize="20" HorizontalAlignment="Left" Margin="10,492,0,0" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeUwaTextBox" Tag="NigeUwa" Text="{Binding Kesan.NigeUwa, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,524,10,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,562,0,0" Text="にげ 上宙1" FontSize="20" VerticalAlignment="Top" Grid.Row="0" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeUwaChu1TextBox" Tag="NigeUwaChu1" Text="{Binding Kesan.NigeUwaChu1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,594,10,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,631,0,0" Text="にげ 上宙2" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeUwaChu2TextBox" Tag="NigeUwaChu2" Text="{Binding Kesan.NigeUwaChu2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,663,10,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,697,0,0" Text="にげ 下宙2" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeShitaChu1TextBox" Tag="NigeShitaChu1" Text="{Binding Kesan.NigeShitaChu1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="9,729,11,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,766,0,0" Text="にげ 下宙1" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeShitaChu2TextBox" Tag="NigeShitaChu2" Text="{Binding Kesan.NigeShitaChu2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="9,798,11,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Margin="10,833,0,0" Text="にげ 下筋" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeShitaTextBox" Tag="NigeShita" Text="{Binding Kesan.NigeShita, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,865,10,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource TitleColor2}" HorizontalAlignment="Left" FontSize="20"  Text="定着板設定" VerticalAlignment="Top" Margin="10,971,0,0" Height="27" Width="100"/>
                <CheckBox x:Name="TeiUwaCheckBox" Tag="teiuwa" IsChecked="{Binding Kesan.Teiuwa, Mode=TwoWay}" Margin="11,1010,0,0"  FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <ContentControl Content="上筋 定着板を使用" FontSize="18" Margin="0,-5"/>
                </CheckBox>
                <CheckBox x:Name="TeiUwaChuCheckBox" Tag="teiuwachu" IsChecked="{Binding Kesan.Teiuwachu, Mode=TwoWay}" Margin="11,1038,0,0"  VerticalAlignment="Top" HorizontalAlignment="Left"  Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <ContentControl Content="上宙 定着板を使用" FontSize="18" Margin="0,-5"/>
                </CheckBox>
                <CheckBox x:Name="TeiShitaChuCheckBox" Tag="teishitachu" IsChecked="{Binding Kesan.Teishitachu, Mode=TwoWay}" Margin="11,1066,0,0"  VerticalAlignment="Top" HorizontalAlignment="Left" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <ContentControl Content="下宙 定着板を使用" FontSize="18" Margin="0,-5"/>
                </CheckBox>
                <CheckBox x:Name="TeiShitaCheckBox" Tag="teishita" IsChecked="{Binding Kesan.Teishita, Mode=TwoWay}" Margin="11,1094,0,0"  VerticalAlignment="Top" HorizontalAlignment="Left" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <ContentControl Content="下筋 定着板を使用" FontSize="18" Margin="0,-5"/>
                </CheckBox>
                <CheckBox x:Name="TeiHarigaibuCheckBox" Tag="teiharigaibu" IsChecked="{Binding Kesan.Teiharigaibu, Mode=TwoWay}" Margin="11,1122,0,0"  VerticalAlignment="Top" HorizontalAlignment="Left" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <ContentControl Content="梁外端部のみに定着板を使用" FontSize="18" Margin="0,-5"/>
                </CheckBox>
                <TextBlock Foreground="{StaticResource TitleColor2}" Margin="10,1152,0,0" Text="主筋アンカ長の指定" FontSize="20" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,1191,0,0" Text="アンカ長 上筋" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top"  />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaUwaTextBox" Tag="AnkaNagaUwa" Text="{Binding Kesan.AnkaNagaUwa, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="9,1223,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Margin="10,1265,0,0" Text="アンカ長 上宙" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top"  />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaUwaChuTextBox" Tag="AnkaNagaUwaChu" Text="{Binding Kesan.AnkaNagaUwaChu, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="9,1297,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Margin="10,1339,0,0" Text="アンカ長 下宙" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaShitaChuTextBox" Text="{Binding Kesan.AnkaNagaShitaChu, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="9,1371,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource Label}" Margin="10,1409,0,0"  Text="アンカ長 下筋" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaShitaTextBox" Tag="AnkaNagaShita" Text="{Binding Kesan.AnkaNagaShita, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,1438,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource TitleColor2}" Margin="10,1493,0,0"  FontSize="20" Text="腹筋にアンカを付ける" VerticalAlignment="Top" Height="32" HorizontalAlignment="Left" Width="297"/>
                <TextBlock Foreground="{StaticResource Label}" Margin="9,1549,0,0"  Text="腹筋 アンカ長" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left"  />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaHaraTextBox" Tag="AnkaNagaHara" Text="{Binding Kesan.AnkaNagaHara, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Margin="10,1578,10,0" FontSize="18" VerticalAlignment="Top" Height="26" />
                <TextBlock Foreground="{StaticResource Label}" Margin="10,1618,0,0"  Text="腹筋 にげ" FontSize="18" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="NigeHaraTextBox" Tag="NigeHara" Text="{Binding Kesan.NigeHara, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Margin="10,1647,10,0" FontSize="18" VerticalAlignment="Top" Height="26"/>
                <TextBlock Foreground="{StaticResource TitleColor1}" FontSize="30" Margin="10,1805,0,0"  Text="アンカ" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBlock Foreground="{StaticResource Label}" HorizontalAlignment="Left" FontSize="20"  Text="アンカ長さ 切り上げの単位(mm)" VerticalAlignment="Top" Margin="10,1853,0,0" Height="27"/>
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaKiriageTextBox" Tag="AnkaNagaKiriage" Text="{Binding Kesan.AnkaNagaKiriage, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="10,1885,10,0"  VerticalAlignment="Top" Height="26" Grid.ColumnSpan="2"/>
                <StackPanel x:Name="RadioButton" Margin="10,1974,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                    <TextBlock Foreground="{StaticResource Label}" FontSize="20"  Text="最低アンカ長さの設定(大梁・基礎梁)" />
                    <RadioButton x:Name="RadioButton1"  Margin="5" 
                                 IsChecked="{Binding SaiteankaOption1, Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="最低アンカ長さを必ず取る" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="RadioButton2"  Margin="5" 
                                 IsChecked="{Binding SaiteankaOption2, Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="直筋定着可能であれば最低アンカは取らない" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                </StackPanel>
                <TextBlock Foreground="{StaticResource Label}" FontSize="20" Margin="10,2082,0,0"  Text="最低アンカ長さ" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaSaiteTextBox" Tag="AnkaNagaSaite" Text="{Binding Kesan.AnkaNagaSaite, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="10,2118,10,0"  VerticalAlignment="Top" Height="26" Grid.ColumnSpan="2"/>
                <TextBlock Foreground="{StaticResource Label}" FontSize="20" Margin="10,2158,0,0"  Text="最上階の上筋のアンカ長さ(単位:d)" VerticalAlignment="Top" Height="27" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="AnkaNagaRFTextBox" Tag="AnkaNagaRF" Text="{Binding Kesan.AnkaNagaRF, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,2190,10,0"  FontSize="18" VerticalAlignment="Top" Height="26" Grid.ColumnSpan="2"/>
                <TextBlock Foreground="{StaticResource TitleColor1}" FontSize="30" Margin="10,2283,0,0"  Text="定着" VerticalAlignment="Top" HorizontalAlignment="Left"/>
                <StackPanel x:Name="harinaitei" Margin="10,2343,0,0" HorizontalAlignment="Left"  VerticalAlignment="Top">
                    <TextBlock Foreground="{StaticResource Label}" FontSize="20" Margin="5"  Text="梁内への直筋定着可不" HorizontalAlignment="Left" Height="auto" Width="auto" />
                    <RadioButton x:Name="harinaitei1"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding HarinaiteiOption1, Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="梁内定着可とする" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="harinaitei2" Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding HarinaiteiOption2,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="梁内定着不可、アンカ定着とする" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="harinaitei3"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" Grid.ColumnSpan="2" 
                                 IsChecked="{Binding HarinaiteiOption3,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="大梁から小梁への梁内定着は不可とする" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                </StackPanel>
                <StackPanel x:Name="shukintei" Margin="10,2539,0,0" HorizontalAlignment="Left"  VerticalAlignment="Top" Grid.ColumnSpan="3">
                    <TextBlock Foreground="{StaticResource Label}" FontSize="20" Margin="5"  Text="主筋の定着(にげが 柱せいの3/4 を超えている場合)" />
                    <RadioButton x:Name="shukintei1"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding ShukinteiOption1,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="柱せいの3/4 まで飲み込んで定着する" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="shukintei2"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding ShukinteiOption2,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="設定したにげ まで飲み込んで定着する" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                </StackPanel>
                <StackPanel x:Name="harinainaga" Margin="10,2719,0,0" HorizontalAlignment="Left"  VerticalAlignment="Top">
                    <TextBlock Foreground="{StaticResource Label}" FontSize="18" Margin="5"  Text="梁内への直筋定着の長さ" HorizontalAlignment="Left" Height="auto" Width="auto" />
                    <RadioButton x:Name="harinainaga1"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding HarinainagaOption1, Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="必ず柱せい+15dの定着を取る" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="harinainaga2"  Margin="5" HorizontalAlignment="Left" Height="auto" Width="auto" 
                                 IsChecked="{Binding HarinainagaOption2,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="定着寸法分のみでよい" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                    <RadioButton x:Name="harinainaga3"  Margin="5" Height="auto" Width="auto" HorizontalAlignment="Left" Grid.ColumnSpan="2" 
                                 IsChecked="{Binding HarinainagaOption3,  Mode=TwoWay}" 
                                 Checked="RadioButton_Checked">
                        <ContentControl Content="最上階は柱せい+15dとし、一般階は定着寸法分のみ" FontSize="18" Margin="0,-5"/>
                    </RadioButton>
                </StackPanel>
                <TextBlock Foreground="{StaticResource Label}" FontSize="20" Margin="10,2974,0,0"  Text="主筋と宙づり筋の間隔(mm)" VerticalAlignment="Top" HorizontalAlignment="Left" />
                <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="ShukinKankakuTextBox" Text="{Binding Kesan.ShukinKankaku, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  FontSize="18" Margin="10,3006,10,0"  VerticalAlignment="Top" Height="26" Grid.ColumnSpan="2"/>
                <TextBlock Foreground="{StaticResource 注意}" FontSize="20" Margin="10,3041,0,0"  Text="※段差がついた場合、引き通し定着が可能かの判定に使用されます" VerticalAlignment="Top" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="27"  />
                <Grid Grid.Column="1" Height="500" Margin="300,115,0,0" VerticalAlignment="Top" Grid.ColumnSpan="3" HorizontalAlignment="Left" Width="900">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="9*"/>
                        <RowDefinition Height="491*"/>
                    </Grid.RowDefinitions>
                    <Canvas Margin="100,91,0,0" x:Name="page1_1_canvas" HorizontalAlignment="Left" Width="603" Height="205" VerticalAlignment="Top" Grid.Row="1"/>
                    <StackPanel Height="auto" Margin="240,16,500,0" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="外端端部" Margin="5" FontSize="18"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="外端端部_textbox" Tag="外端端部" Text="{Binding Kesan.GaitanTanbuTopYochou, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="5" Width="60" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Margin="545,16,140,0" Height="auto" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="內端端部" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="內端端部_textbox" Tag="內端端部" Text="{Binding Kesan.NaitanTanbuTopYochou, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="5" Width="60" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Height="auto" Margin="240,391,500,0" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="外端中央部" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="外端中央部_textbox" Tag="外端中央部" Text="{Binding Kesan.GaitanChubuTopYochou, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Width="60" Margin="5" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Margin="545,391,140,0" Height="auto" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="內端中央部" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="內端中央部_textbox" Tag="內端中央部" Text="{Binding Kesan.NaitanChubuTopYochou, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Width="60" Margin="5" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                </Grid>
                <Canvas Grid.Column="1" Height="282" Margin="34,984,0,0" x:Name="myCanvas" VerticalAlignment="Top" HorizontalAlignment="Left" Width="458"/>
                <Grid Grid.Column="1" HorizontalAlignment="Left" Height="500" Margin="300,1165,0,0" VerticalAlignment="Top" Width="900" Grid.ColumnSpan="3">
                    <Canvas Margin="100,91,0,0" x:Name="page1_3_canvas" HorizontalAlignment="Left" Width="603" Height="205" VerticalAlignment="Top" Grid.Row="1"/>
                    <StackPanel Height="auto" Margin="240,16,400,0" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="外端中央" Margin="5" FontSize="18"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="外端中央3_textbox" Text="{Binding Kesan.GaitanChubuTopYochou3, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="5" Width="60" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Margin="545,16,140,0" Height="auto" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="內端中央" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="內端中央3_textbox" Text="{Binding Kesan.NaitanChubuTopYochou3, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Margin="5" Width="60" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Height="auto" Margin="240,391,400,0" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="外端端部" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="トップ余長" FontSize="15" Width="60" Height="20"/>
                            <TextBox InputMethod.IsInputMethodEnabled="False" x:Name="外端端部3_textbox" Text="{Binding Kesan.GaitanTanbuTopYochou3, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="18" Width="60" Margin="5" HorizontalAlignment="Left" TextAlignment="Right"/>
                            <TextBlock Text="d" FontSize="18" Width="19" Height="25"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Margin="545,391,140,0" Height="auto" VerticalAlignment="Top" Grid.Row="1">
                        <TextBlock Text="內端端部" FontSize="18" Margin="5"/>
                        <StackPanel Orientation="Horizontal">
